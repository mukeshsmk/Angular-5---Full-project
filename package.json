{
  "name": "app-retail",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "prod.build": "ng build --prod --aot",
    "prod.clean.s3": "aws s3 rm s3://appretail.io --recursive",
    "prod.cp.to.s3": "aws s3 cp ./dist/app-retail s3://appretail.io --recursive --cache-control max-age=31556926",
    "prod.cp.index.to.s3": "aws s3api put-object --bucket appretail.io --key index.html --body ./dist/app-retail/index.html --cache-control max-age=10 --content-type 'text/html'",
    "prod.invalidate.cloudfront": "aws cloudfront create-invalidation --distribution-id E366K2K9975Y5U --paths /index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/jest": "^24.0.13",
    "angular-calendar": "^0.27.8",
    "bootstrap": "4.3.1",
    "chart.js": "^2.9.4",
    "core-js": "^2.5.4",
    "date-fns": "^1.30.1",
    "font-awesome": "^4.7.0",
    "jquery": "3.2.1",
    "popper.js": "1.14.7",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
